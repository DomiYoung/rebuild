  "scripts": {
    "dev": "cross-env BABEL_ENV=development NODE_OPTIONS=--max_old_space_size=8192 webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "build:master": "cross-env NODE_ENV=production env_config=prod node --max-old-space-size=4096  build/build.js",
    "build:prod1": "cross-env NODE_ENV=production env_config=prod1 node --max-old-space-size=4096  build/build.js",
    "build:dev": "cross-env NODE_ENV=development env_config=dev node --max-old-space-size=4096 build/build.js",
    "build:uat": "cross-env NODE_ENV=production env_config=sit node --max-old-space-size=8192 build/build.js",
    "build:test": "cross-env NODE_ENV=production env_config=test node --max-old-space-size=4096  build/build.js",
